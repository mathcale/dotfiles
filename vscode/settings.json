{
  "[html]": {
    "editor.defaultFormatter": "vscode.html-language-features"
  },
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[json]": {
    "editor.defaultFormatter": "vscode.json-language-features"
  },
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "aws.telemetry": false,
  "breadcrumbs.enabled": true,
  "diffEditor.ignoreTrimWhitespace": false,
  "editor.bracketPairColorization.enabled": true,
  "editor.cursorBlinking": "smooth",
  "editor.cursorSmoothCaretAnimation": "on",
  "editor.cursorStyle": "line",
  "editor.fontFamily": "'Fira Code'",
  "editor.fontLigatures": true,
  "editor.fontSize": 18,
  "editor.formatOnPaste": false,
  "editor.formatOnSave": false,
  "editor.inlineSuggest.enabled": true,
  "editor.largeFileOptimizations": false,
  "editor.linkedEditing": true,
  "editor.minimap.renderCharacters": false,
  "editor.renderWhitespace": "all",
  "editor.rulers": [
    100
  ],
  "editor.smoothScrolling": true,
  "editor.snippetSuggestions": "top",
  "editor.stickyScroll.enabled": false,
  "editor.suggestSelection": "first",
  "editor.wordWrap": "on",
  "emmet.includeLanguages": {
    "erb": "html",
    "javascript": "javascriptreact",
    "phtml": "html",
    "twig": "html"
  },
  "emmet.syntaxProfiles": {
    "javascript": "jsx"
  },
  "errorLens.exclude": [
    "never (read|used)"
  ],
  "eslint.validate": [
    "javascript",
    "javascriptreact",
    "vue"
  ],
  "explorer.confirmDelete": false,
  "explorer.confirmDragAndDrop": false,
  "explorer.fileNesting.enabled": true,
  "explorer.fileNesting.expand": true,
  "explorer.fileNesting.patterns": {
    "*.js": "${capture}.js.map, ${capture}.min.js, ${capture}.d.ts",
    "*.jsx": "${capture}.js",
    "*.test.js": "${capture}.test.js",
    "*.test.jsx": "${capture}.test.jsx",
    "*.test.ts": "${capture}.test.ts",
    "*.test.tsx": "${capture}.test.tsx",
    "*.ts": "${capture}.js",
    "*.tsx": "${capture}.ts",
    "package.json": "",
    "tsconfig.json": "tsconfig.*.json"
  },
  "files.exclude": {
    "**/.classpath": true,
    "**/.factorypath": true,
    "**/.project": true,
    "**/.settings": true
  },
  "files.watcherExclude": {
    "**/.git/objects/**": true,
    "**/.git/subtree-cache/**": true,
    "**/node_modules/*/**": true
  },
  "git.confirmSync": false,
  "github.copilot.enable": {
    "*": true,
    "plaintext": false,
    "markdown": true,
    "scminput": false,
    "typescriptreact": true,
    "yaml": false
  },
  "githubPullRequests.createOnPublishBranch": "never",
  "githubPullRequests.pullBranch": "never",
  "gitlens.advanced.messages": {
    "suppressFileNotUnderSourceControlWarning": true,
    "suppressGitMissingWarning": true
  },
  "gitlens.telemetry.enabled": false,
  "go.formatTool": "goimports",
  "go.lintOnSave": "file",
  "go.testFlags": [
    "-v"
  ],
  "go.toolsManagement.autoUpdate": true,
  "java.jdt.ls.vmargs": "-XX:+UseParallelGC -XX:GCTimeRatio=4 -XX:AdaptiveSizePolicyWeight=90 -Dsun.zip.disableMemoryMapping=true -Xmx1G -Xms100m",
  "java.project.importOnFirstTimeStartup": "automatic",
  "javascript.updateImportsOnFileMove.enabled": "always",
  "js/ts.implicitProjectConfig.checkJs": true,
  "picowgo.pythonPath": "python3",
  "react-native-tools.showUserTips": false,
  "redhat.telemetry.enabled": false,
  "search.useIgnoreFiles": true,
  "security.workspace.trust.banner": "never",
  "security.workspace.trust.enabled": false,
  "security.workspace.trust.untrustedFiles": "open",
  "sqltools.useNodeRuntime": true,
  "svg.preview.mode": "svg",
  "telemetry.telemetryLevel": "off",
  "terminal.external.osxExec": "iTerm.app",
  "terminal.integrated.defaultProfile.linux": "bash",
  "terminal.integrated.fontFamily": "FiraCode Nerd Font Mono",
  "terminal.integrated.fontSize": 14,
  "terminal.integrated.profiles.linux": {
    "bash": {
      "args": [
        "--host",
        "--env=TERM=xterm-256color",
        "zsh"
      ],
      "path": "/usr/bin/flatpak-spawn"
    }
  },
  "terminal.integrated.smoothScrolling": true,
  "typescript.updateImportsOnFileMove.enabled": "always",
  "vsicons.dontShowNewVersionMessage": true,
  "window.commandCenter": true,
  "window.menuBarVisibility": "visible",
  "window.titleBarStyle": "custom",
  "workbench.activityBar.visible": true,
  "workbench.colorTheme": "Catppuccin Mocha",
  "workbench.iconTheme": "vscode-icons",
  "workbench.startupEditor": "none",
  "yaml.customTags": [
    "!Equals sequence",
    "!FindInMap sequence",
    "!GetAtt",
    "!GetAZs",
    "!ImportValue",
    "!Join sequence",
    "!Ref",
    "!Select sequence",
    "!Split sequence",
    "!Sub"
],
  "debug.javascript.autoAttachFilter": "disabled",
  "extensions.ignoreRecommendations": true,
  "[jsonc]": {
    "editor.defaultFormatter": "vscode.json-language-features"
  },
}
